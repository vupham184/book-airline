<?xml version="1.0" encoding="utf-8"?>
<extension version="2.5" type="module" client="administrator" method="upgrade">
    <name>mod_myeditor</name>
	<version>2.5.1</version>
	<creationDate>2.5.1</creationDate>
	<author>http://compojoom.com</author>
	<authorEmail>support@joomlapraise.com</authorEmail>
	<authorUrl>http://www.joomlapraise.com</authorUrl>
	<copyright>JoomlaPraise</copyright>
	<license>GNU/GPL</license>
	<description>MY_EDITOR_FOR_JOOMLA</description>
    <files>
        <folder>assets</folder>
<file>helper.php</file>
<file>index.html</file>
<file module="mod_myeditor">mod_myeditor.php</file>
<folder>tmpl</folder>
    </files>
    <languages folder="administrator/language">
        <language tag="da-DK">da-DK/da-DK.mod_myeditor.ini</language>
<language tag="de-DE">de-DE/de-DE.mod_myeditor.ini</language>
<language tag="en-GB">en-GB/en-GB.mod_myeditor.ini</language>
<language tag="et-EE">et-EE/et-EE.mod_myeditor.ini</language>
<language tag="hu-HU">hu-HU/hu-HU.mod_myeditor.ini</language>
<language tag="nl-NL">nl-NL/nl-NL.mod_myeditor.ini</language>
<language tag="pt-PT">pt-PT/pt-PT.mod_myeditor.ini</language>
<language tag="sv-SE">sv-SE/sv-SE.mod_myeditor.ini</language>
<language tag="th-TH">th-TH/th-TH.mod_myeditor.ini</language>
    </languages>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="moduleclass_sfx"
					type="text"
					default=""
					label="MODULE CLASS SUFFIX"
					description="PARAMMODULECLASSSUFFIX" />
				<field
					id="mod_myeditor"
					name="mod_myeditor"
					type="cmsmarketconnect"
					label="Version Information"
					description="Up to date version, support, and review information from CMS Market."
					default="" />
			</fieldset>
		</fields>
	</config>

</extension>
